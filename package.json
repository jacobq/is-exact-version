{
  "name": "is-exact-version",
  "version": "0.1.3",
  "description": "Simple, modern module for identifying whether a package.json \"version range\" string refers to an exact version",
  "files": [
    "dist"
  ],
  "main": "dist/is-exact-version.js",
  "types": "dist/is-exact-version.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "prepare": "npm run build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jacobq/is-exact-version.git"
  },
  "keywords": [
    "exact",
    "semver",
    "version",
    "range"
  ],
  "author": "Jacob Quant <jacobq@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jacobq/is-exact-version/issues"
  },
  "homepage": "https://github.com/jacobq/is-exact-version#readme",
  "engines": {
    "node": "8.* || 10.* || >= 12.*"
  },
  "dependencies": {
    "@commitlint/travis-cli": "8.1.0",
    "debug": "4.1.1",
    "semver": "6.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "8.1.0",
    "@commitlint/config-conventional": "8.1.0",
    "@types/debug": "4.1.4",
    "@types/jest": "24.0.15",
    "@types/semver": "6.0.1",
    "codecov": "3.5.0",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "6.1.0",
    "eslint-config-airbnb-base": "13.2.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.14.1",
    "husky": "3.0.3",
    "jest": "24.8.0",
    "lint-staged": "9.2.1",
    "ts-jest": "24.0.2",
    "typescript": "3.5.3",
    "typescript-eslint-parser": "22.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged && npm run test"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{ts,js}": [
        "eslint --fix",
        "git add"
      ]
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
