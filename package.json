{
  "name": "is-exact-version",
  "version": "2.0.0-beta.1",
  "description": "Simple, modern module for identifying whether a package.json \"version range\" string refers to an exact version",
  "files": [
    "dist"
  ],
  "main": "dist/is-exact-version.js",
  "type": "module",
  "types": "dist/is-exact-version.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "prepare": "npm run build",
    "test": "cross-env NODE_OPTIONS=\"--experimental-vm-modules\" jest"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jacobq/is-exact-version.git"
  },
  "keywords": [
    "exact",
    "semver",
    "version",
    "range"
  ],
  "author": "Jacob Quant <jacobq@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jacobq/is-exact-version/issues"
  },
  "homepage": "https://github.com/jacobq/is-exact-version#readme",
  "engines": {
    "node": ">= 12.*"
  },
  "dependencies": {
    "semver": "^7.3.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^19.0.0",
    "@commitlint/travis-cli": "^19.0.0",
    "@types/debug": "^4.1.7",
    "@types/jest": "^27.4.1",
    "@types/semver": "^7.3.9",
    "@typescript-eslint/parser": "^5.14.0",
    "codecov": "^3.8.2",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.5",
    "ts-jest": "^27.1.3",
    "typescript": "^4.7.0-dev.20220308"
  },
  "peerDependencies": {
    "debug": "^4.3.3"
  },
  "peerDependenciesMeta": {
    "debug": {
      "optional": true
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged && npm run test"
    }
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
