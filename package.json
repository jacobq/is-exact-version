{
  "name": "is-exact-version",
  "version": "1.0.0",
  "description": "Simple, modern module for identifying whether a package.json \"version range\" string refers to an exact version",
  "files": [
    "dist"
  ],
  "main": "dist/is-exact-version.js",
  "types": "dist/is-exact-version.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "prepare": "npm run build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jacobq/is-exact-version.git"
  },
  "keywords": [
    "exact",
    "semver",
    "version",
    "range"
  ],
  "author": "Jacob Quant <jacobq@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jacobq/is-exact-version/issues"
  },
  "homepage": "https://github.com/jacobq/is-exact-version#readme",
  "engines": {
    "node": "10.* || >= 12.*"
  },
  "dependencies": {
    "@commitlint/travis-cli": "8.3.4",
    "debug": "4.1.1",
    "semver": "7.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.4",
    "@commitlint/config-conventional": "8.3.4",
    "@types/debug": "4.1.5",
    "@types/jest": "24.0.25",
    "@types/semver": "6.2.0",
    "codecov": "3.6.1",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-jest": "23.3.0",
    "husky": "4.0.4",
    "jest": "24.9.0",
    "lint-staged": "9.5.0",
    "ts-jest": "24.3.0",
    "typescript": "3.7.4",
    "typescript-eslint-parser": "22.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged && npm run test"
    }
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
