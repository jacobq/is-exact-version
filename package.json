{
  "name": "is-exact-version",
  "version": "0.1.2",
  "description": "Simple, modern module for identifying whether a package.json \"version range\" string refers to an exact version",
  "files": [
    "dist"
  ],
  "main": "dist/is-exact-version.js",
  "types": "dist/is-exact-version.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "prepare": "npm run build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jacobq/is-exact-version.git"
  },
  "keywords": [
    "exact",
    "semver",
    "version",
    "range"
  ],
  "author": "Jacob Quant <jacobq@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jacobq/is-exact-version/issues"
  },
  "homepage": "https://github.com/jacobq/is-exact-version#readme",
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "dependencies": {
    "@commitlint/travis-cli": "7.2.1",
    "debug": "4.1.0",
    "semver": "5.6.0"
  },
  "devDependencies": {
    "@commitlint/cli": "7.2.1",
    "@commitlint/config-conventional": "7.1.2",
    "@types/debug": "0.0.31",
    "@types/jest": "23.3.9",
    "@types/semver": "5.5.0",
    "codecov": "3.1.0",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "5.9.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jest": "22.0.0",
    "husky": "1.1.4",
    "jest": "23.6.0",
    "lint-staged": "8.0.4",
    "ts-jest": "23.10.4",
    "typescript": "3.1.6",
    "typescript-eslint-parser": "21.0.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged && npm run test"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{ts,js}": [
        "eslint --fix",
        "git add"
      ]
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
